      <field name="name">ibschool.dashboard_kpi tree</field>
      <field name="model">ibschool.dashboard_kpi</field>
      <field name="arch" type="xml">
        <tree string="ibschool.dashboard_kpi">
          <field name="name"/>
        </tree>
      </field>
