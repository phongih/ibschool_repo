<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>
    <!-- Action & Views for ibschool.fee_config -->
    <record id="view_ibschool.fee_config_tree" model="ir.ui.view">
      <field name="name">ibschool.fee_config tree</field>
      <field name="model">ibschool.fee_config</field>
      <field name="arch" type="xml">
        <tree string="ibschool.fee_config">
          <field name="name"/>
        </tree>
      </field>
    </record>

    <record id="view_ibschool.fee_config_form" model="ir.ui.view">
      <field name="name">ibschool.fee_config form</field>
      <field name="model">ibschool.fee_config</field>
      <field name="arch" type="xml">
        <form string="ibschool.fee_config">
          <sheet>
            <group>
              <field name="name"/>
            </group>
          </sheet>
        </form>
      </field>
    </record>

    <record id="action_ibschool.fee_config" model="ir.actions.act_window">
      <field name="name">ibschool.fee_config</field>
      <field name="res_model">ibschool.fee_config</field>
      <field name="view_mode">tree,form</field>
      <field name="help" type="html"><![CDATA[Manage ibschool.fee_config records.]]></field>
    </record>
  </data>
</odoo>
