<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>
    <!-- Action & Views for ibschool.ai_knowledge -->
    <record id="view_ibschool.ai_knowledge_tree" model="ir.ui.view">
      <field name="name">ibschool.ai_knowledge tree</field>
      <field name="model">ibschool.ai_knowledge</field>
      <field name="arch" type="xml">
        <tree string="ibschool.ai_knowledge">
          <field name="name"/>
        </tree>
      </field>
    </record>

    <record id="view_ibschool.ai_knowledge_form" model="ir.ui.view">
      <field name="name">ibschool.ai_knowledge form</field>
      <field name="model">ibschool.ai_knowledge</field>
      <field name="arch" type="xml">
        <form string="ibschool.ai_knowledge">
          <sheet>
            <group>
              <field name="name"/>
            </group>
          </sheet>
        </form>
      </field>
    </record>

    <record id="action_ibschool.ai_knowledge" model="ir.actions.act_window">
      <field name="name">ibschool.ai_knowledge</field>
      <field name="res_model">ibschool.ai_knowledge</field>
      <field name="view_mode">tree,form</field>
      <field name="help" type="html"><![CDATA[Manage ibschool.ai_knowledge records.]]></field>
    </record>
  </data>
</odoo>
