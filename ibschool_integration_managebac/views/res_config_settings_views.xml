<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>
    <!-- Action & Views for ibschool.res_config_settings -->
    <record id="view_ibschool.res_config_settings_tree" model="ir.ui.view">
      <field name="name">ibschool.res_config_settings tree</field>
      <field name="model">ibschool.res_config_settings</field>
      <field name="arch" type="xml">
        <tree string="ibschool.res_config_settings">
          <field name="name"/>
        </tree>
      </field>
    </record>

    <record id="view_ibschool.res_config_settings_form" model="ir.ui.view">
      <field name="name">ibschool.res_config_settings form</field>
      <field name="model">ibschool.res_config_settings</field>
      <field name="arch" type="xml">
        <form string="ibschool.res_config_settings">
          <sheet>
            <group>
              <field name="name"/>
            </group>
          </sheet>
        </form>
      </field>
    </record>

    <record id="action_ibschool.res_config_settings" model="ir.actions.act_window">
      <field name="name">ibschool.res_config_settings</field>
      <field name="res_model">ibschool.res_config_settings</field>
      <field name="view_mode">tree,form</field>
      <field name="help" type="html"><![CDATA[Manage ibschool.res_config_settings records.]]></field>
    </record>
  </data>
</odoo>
